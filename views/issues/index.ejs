<h2>Search Issues</h2> 
<form action="/issues" method="GET"> 
  <div>
    <label>Title</label> 
    <input type="text" name="title" value="<% searchOptions.title %>"> 
  </div>
  <button type="submit">Search</button>
</form>
<br>
<div>
  <% issues.forEach( function(issue) {  %>
  <a href="/issues/<%= issue.id %>">   
    <div><%= issue.title %></div>
  </a>
  <% }) %>
</div>

